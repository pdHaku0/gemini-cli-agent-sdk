{
  "name": "@pdhaku0/gemini-cli-agent-sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "A high-level SDK for building AI Agent chat interfaces using Gemini ACP",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "client",
    "server",
    "docs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./extras": {
      "types": "./dist/extras/index.d.ts",
      "default": "./dist/extras/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && tsc && node scripts/postbuild.mjs",
    "postbuild": "node scripts/postbuild.mjs",
    "dev": "tsc -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "start:bridge": "export GEMINI_PORT=4444 && node scripts/gemini-bridge.cjs",
    "prepare": "npm run build"
  },
  "keywords": [
    "gemini",
    "acp",
    "agent",
    "chat",
    "sdk",
    "ai"
  ],
  "author": "薄明色の忘れ路",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "diff": "^5.2.0",
    "events": "^3.3.0",
    "ws": "^8.19.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/diff": "^5.2.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.18.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.4.0"
  }
}
