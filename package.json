{
  "name": "@yohaku/gemini-cli-agent-sdk",
  "version": "0.1.3",
  "type": "module",
  "description": "A high-level SDK for building AI Agent chat interfaces using Gemini ACP",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "client",
    "server"
  ],
  "scripts": {
    "build": "tsc && node scripts/postbuild.mjs",
    "postbuild": "node scripts/postbuild.mjs",
    "dev": "tsc -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "start:bridge": "export GEMINI_PORT=4444 && node scripts/gemini-bridge.cjs",
    "start:cli": "export GEMINI_WS_URL=ws://localhost:4444 && node dist/test-cli.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "gemini",
    "acp",
    "agent",
    "chat",
    "sdk",
    "ai"
  ],
  "author": "薄明色の忘れ路",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.23.0",
    "events": "^3.3.0",
    "diff": "^5.2.0"
  },
  "devDependencies": {
    "@types/diff": "^5.2.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.4.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.0",
    "ws": "^8.17.0"
  }
}