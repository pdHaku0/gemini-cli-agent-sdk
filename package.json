{
  "name": "@yohaku/gemini-cli-agent-sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "A high-level SDK for building AI Agent chat interfaces using Gemini ACP",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "start:bridge": "export GEMINI_PORT=4444 && node scripts/gemini-bridge.cjs",
    "start:cli": "export GEMINI_WS_URL=ws://localhost:4444 && node dist/test-cli.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "gemini",
    "acp",
    "agent",
    "chat",
    "sdk",
    "ai"
  ],
  "author": "薄明色の忘れ路",
  "license": "MIT",
  "dependencies": {
    "ws": "^8.18.0",
    "zod": "^3.23.0",
    "events": "^3.3.0",
    "diff": "^5.2.0"
  },
  "devDependencies": {
    "@types/diff": "^5.2.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.4.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.0"
  }
}